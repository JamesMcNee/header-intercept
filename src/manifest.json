{
  "name": "Header Intercept",
  "version": "0.3.0",
  "version_name": "0.3.0 BETA",
  "description": "Add custom headers to requests.",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "assets/icon/16.png",
    "48": "assets/icon/48.png",
    "128": "assets/icon/128.png"
  },
  "background": {
    "scripts": [
        "background.bundle.js",
        "bundle.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
